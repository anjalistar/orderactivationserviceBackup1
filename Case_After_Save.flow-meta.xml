            <conditions>
                <leftValueReference>$Record.Tech_Survey_Sent__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Tech_Survey_Received__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Opportunity_Tech_Survey_Checkboxes</targetReference>
            </connector>
            <label>Tech Survey Check Criteria</label>
        </rules>
    </decisions>
