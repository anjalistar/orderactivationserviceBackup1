            </conditions>
            <connector>
                <targetReference>Publish_Agency_Pro_Assigned_Tam_Slack_PE</targetReference>
            </connector>
            <label>Agency Pro Active</label>
        </rules>
    </decisions>
    <decisions>
        <name>Decision_Sub_Plan_Name</name>
        <label>Decision Sub Plan Name</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Check_if_Plan_is_Dedicated</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Agency_Pro</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.WPE_Subscription_plan__r.WPE_Subscription_Family__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Agency Pro</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_OpportunityLine_Items</targetReference>
            </connector>
            <label>Agency Pro</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Opportunity_Split</name>
        <label>Is the Opportunity Split?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Opportunity is Split</defaultConnectorLabel>
        <rules>
            <name>Opportunity_is_not_Split</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Split_Opportunity_Lookup__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Shared_Plan_list</targetReference>
            </connector>
            <label>Opportunity is not Split</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Plan_Dedicated</name>
        <label>Is Plan Dedicated?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Compare_Opportunity_Plan_to_Shared_Plan_List</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Plan is Dedicated</defaultConnectorLabel>
        <rules>
            <name>Plan_is_Shared</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Compare_Opportunity_Plan_to_Shared_Plan_List.Label</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Subscription_Family__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set</targetReference>
            </connector>
            <label>Plan is Shared</label>
        </rules>
    </decisions>
    <decisions>
        <name>Opportunity_Type</name>
        <label>Opportunity Type?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Is_Opportunity_Split</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Premium</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.WPE_Subscription_plan__r.Subscription_Family_Map__r.CX_Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Premium</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Premium_Plus_Indicator__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Premium_Plus_Indicator__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Slack_Notification_Is_Active_Premium</targetReference>
            </connector>
            <label>Premium</label>
        </rules>
        <rules>
            <name>Enterprise</name>
            <conditionLogic>1 OR (2 AND 3)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.WPE_Subscription_plan__r.Subscription_Family_Map__r.CX_Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Enterprise</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.WPE_Subscription_plan__r.Subscription_Family_Map__r.CX_Plan_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Premium</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Premium_Plus_Indicator__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Slack_Notification_Is_Active_Enterprise</targetReference>
            </connector>
            <label>Enterprise</label>
        </rules>
    </decisions>
    <description>ESS1913 - DON - Added Decision to check for Split Opp and avoid firing if true</description>
