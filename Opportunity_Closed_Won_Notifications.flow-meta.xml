<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>62.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Set</name>
        <label>Set Plan is Shared Flag</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>PlanIsShared</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </assignmentItems>
        <connector>
            <isGoTo>true</isGoTo>
            <targetReference>Check_if_Plan_is_Dedicated</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Check_if_Plan_is_Dedicated</name>
        <label>Check if Plan is Dedicated</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Shared Plan - No Slack Notification Required</defaultConnectorLabel>
        <rules>
            <name>Dedicated_Plan</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PlanIsShared</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Publish_Dedicated_Closed_Won_Slack_Platform_Event</targetReference>
            </connector>
            <label>Dedicated Plan</label>
        </rules>
    </decisions>
    <decisions>
