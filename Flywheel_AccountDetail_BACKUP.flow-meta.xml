    <decisions>
        <description>IF flywheel_user_id exists in Account, then UPDATE, else ADD Account record</description>
        <name>Add_Update_Account</name>
        <label>Add_Update_Account</label>
        <locationX>182</locationX>
        <locationY>455</locationY>
        <defaultConnector>
            <targetReference>Account_Update</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Update_dec</defaultConnectorLabel>
        <rules>
            <name>Add_dec</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Search_Account_flywheel_user_id.Name</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Account_Add</targetReference>
            </connector>
            <label>Add_dec</label>
        </rules>
