    <formulas>
        <name>calOppName</name>
        <dataType>String</dataType>
        <expression>{!GetOpportunities.Account.Name}+&quot; - &quot;+{!recordTypeName}+&quot; - &quot;+TEXT({!GetOpportunities.CloseDate})</expression>
    </formulas>
    <formulas>
        <name>formulaDateTimeDifference</name>
        <dataType>Number</dataType>
        <expression>({!GetOpportunities.LastModifiedDate}-{!GetOpportunities.CreatedDate})*24*60*60</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>TeamRole</name>
        <dataType>String</dataType>
        <expression>IF(
ISPICKVAL({!LeadRecord.Lead_Team__c}, &apos;Other&apos;),
&apos;SDR&apos;, 
TEXT({!LeadRecord.Lead_Team__c})
)</expression>
    </formulas>
