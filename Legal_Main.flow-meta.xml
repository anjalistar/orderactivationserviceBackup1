            <assignToReference>PriorityRenumberCollection</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>Renumber_Loop</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Renumber_Loop</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Set_new_Priority</name>
        <label>Set new Priority</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>Renumber_Loop.Priority__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Renumber_Loop.Priority_Helper__c</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Add_to_Renumber_Collection</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Any_records_with_same_Priority</name>
        <label>Any records with same Priority?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Let_s_Renumber</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Check_for_Records_with_Same_Priority</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Renumber_Loop</targetReference>
            </connector>
            <label>Yes. Let&apos;s Renumber</label>
        </rules>
    </decisions>
    <decisions>
        <name>Has_Opportunity</name>
        <label>Has Opportunity?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Priority_Updated</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Has_Opportunity</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Opportunity__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Populate_Account_ID</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>IF_Priority_is_Blank_then_set_to_Picklist_None</name>
        <label>IF Priority is Blank, then set to Picklist None</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Set_Legal_Record_Owner_if_Legal_field_on_Opp_is_Populated</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No change</defaultConnectorLabel>
        <rules>
            <name>Priority_is_Blank</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Priority__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Priority_to_None</targetReference>
            </connector>
            <label>Priority is Blank</label>
        </rules>
    </decisions>
    <decisions>
        <name>Priority_Updated</name>
        <label>Priority Updated?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Priority_Updated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PriorityIsNewIsChanged</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_for_Records_with_Same_Priority</targetReference>
            </connector>
            <label>Yes Priority Updated</label>
        </rules>
    </decisions>
    <decisions>
        <name>Set_Legal_Record_Owner_if_Legal_field_on_Opp_is_Populated</name>
        <label>Set Legal Record Owner if Legal field on Opp is Populated</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Has_Opportunity</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Change</defaultConnectorLabel>
        <rules>
            <name>Match_Legal_on_Opp_to_Legal_Record_Owner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Legal_Reviewer_on_Opp__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OwnerId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Legal_Review_on_Opp_ID__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Opportunity__r.Legal__r.IsActive</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Legal_Record_Owner</targetReference>
            </connector>
            <label>Match Legal on Opp to Legal Record Owner</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>PriorityIsNewIsChanged</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW() || ISCHANGED({!$Record.Priority__c})</expression>
    </formulas>
    <interviewLabel>Legal Main {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Legal Main</label>
    <loops>
        <name>Renumber_Loop</name>
        <label>Renumber Loop</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <collectionReference>Check_for_Records_with_Same_Priority</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Set_new_Priority</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Priority_Renumber</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
