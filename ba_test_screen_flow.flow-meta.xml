    <formulas>
        <name>absresutls</name>
        <dataType>Number</dataType>
        <expression>BLANKVALUE({!recOppIn.Subscription_Current_USD_Price__c},0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>calcnestedIf</name>
        <dataType>Number</dataType>
        <expression>IF(
{!recOppIn.Short_Term_Project__c},
(BLANKVALUE({!recOppIn.Opportunity_USD_Price__c},0) - BLANKVALUE({!recOppIn.Subscription_Current_USD_Price__c},0)) / 12,
BLANKVALUE({!recOppIn.Opportunity_USD_Price__c},0) - BLANKVALUE({!recOppIn.Subscription_Current_USD_Price__c},0)
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>subUSDNull</name>
