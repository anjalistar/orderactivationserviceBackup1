    <formulas>
        <name>calcAddOnSOQL</name>
        <dataType>String</dataType>
        <expression>&quot;Select Id FROM Onboarding_Template__c WHERE Add_Ons__c INCLUDES (&apos;&quot; +{!loop_Add_Ons} + &quot;&apos;) AND Onboarding_Type__c = &apos;&quot; + {!recOppRec_In.Onboarding_Type_NEW__c} + &quot;&apos; AND Effective_From__c &lt;= TODAY AND Effective_To__c &gt;= TODAY&quot;</expression>
    </formulas>
    <formulas>
        <name>calcOBTemplateSOQL</name>
        <dataType>String</dataType>
        <expression>&quot;SELECT Id FROM Onboarding_Template__c WHERE Add_On_Product__c IN (Select Product2Id FROM OpportunityLineItem WHERE OpportunityId = &apos;&quot; + {!recOppRec_In.Id} + &quot;&apos;) AND Onboarding_Type__c= &apos;&quot; + {!recOppRec_In.Onboarding_Type_NEW__c} + &quot;&apos; AND Effective_From__c &lt;= TODAY AND Effective_To__c &gt;= TODAY&quot;</expression>
    </formulas>
    <formulas>
        <name>calcOppProdSOQL</name>
        <dataType>String</dataType>
        <expression>&quot;SELECT Product2Id, Name, ProductCode FROM OpportunityLineItem WHERE OpportunityId = &apos;&quot; + {!recOppRec_In.Id} + &quot;&apos; AND Catalog_Type__c = &apos;Add-On&apos;&quot;</expression>
    </formulas>
    <formulas>
        <name>calcPlanTypeSOQL</name>
        <dataType>String</dataType>
        <expression>&quot;Select Id FROM Onboarding_Template__c WHERE Plan_Types__c INCLUDES (&apos;&quot; + {!loop_OB_Plan_Types} + &quot;&apos;) AND Onboarding_Type__c = &apos;&quot; + {!recOppRec_In.Onboarding_Type_NEW__c} + &quot;&apos; AND Effective_From__c &lt;= TODAY AND Effective_To__c &gt;= TODAY&quot;</expression>
    </formulas>
